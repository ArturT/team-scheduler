<h1>List Of Projects</h1>
<ul>
   <% @projects.each do |project| %>
      <li>
         <%= project.name %>
         <%= link_to 'edit', edit_project_path(project) %>
         <%= link_to 'delete', project, method: :delete %>
      </li>
   <% end %>
</ul>
<%= link_to 'Add New Project', new_project_path %>
